{
    "name": "aurmil/slim3-skeleton",
    "description": "A Slim Framework 3 application skeleton",
    "type": "project",
    "keywords": ["slimphp", "slim", "framework"],
    "homepage": "https://github.com/aurmil/slim3-skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "AurÃ©lien Millet",
            "homepage": "http://www.aurelien-millet.fr/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "akrabat/rka-slim-session-middleware": "^2.0",
        "monolog/monolog": "^1.21",
        "roave/security-advisories": "dev-master",
        "slim/csrf": "^0.8.1",
        "slim/flash": "^0.2.0",
        "slim/slim": "^3.5",
        "slim/twig-view": "^2.2",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/yaml": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "*",
        "symfony/var-dumper": "^3.4"
    }
}
